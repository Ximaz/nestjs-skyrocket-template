{
  "name": "nestjs-skyrocket-template",
  "version": "0.0.1",
  "description": "A NestJS template made to make it faster.",
  "author": "DURAND Malo <durand.malo.60590@gmail.com> (Ximaz)",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "start": "dotenv -e .env.local -- nest start",
    "start:dev": "dotenv -e .env.local -- nest start --watch",
    "start:debug": "dotenv -e .env.local -- nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --check src/",
    "format:fix": "prettier --write src/",
    "pretest": "pnpm build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:debug": "NODE_OPTIONS=--experimental-vm-modules node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.local -- prisma studio"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/static": "^8.2.0",
    "@keyv/redis": "^5.1.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.17.0",
    "@redis/client": "^5.8.3",
    "cacheable": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-env": "^10.1.0",
    "fastify": "^5.6.1",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.5.3",
    "nest-winston": "^1.10.2",
    "prisma": "^6.17.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-themes": "^1.4.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.8",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.5",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.18.8",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^10.0.0",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-esm-transformer": "^1.0.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "pnpm": {
    "overrides": {
      "fastify": "5.6.1"
    }
  }
}
